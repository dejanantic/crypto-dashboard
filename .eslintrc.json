{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
      }
    ],
    "func-names": "off",
    "no-undef": "warn",
    "no-unused-vars": "warn"
  }
}
